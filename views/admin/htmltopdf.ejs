
<div class="work-space  pt-2 px-3 ps-5">


    <h4 class="text-center mt-2">Total Orders</h4>
    <h5><%=count %></h5>


    

    <div class="container bootstrap snippets bootdey mt-3">
        <div class="row">
            <div class="col-lg-12">
                <div class="main-box no-header clearfix">
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table user-list">
                                <thead>
                                    <tr>
                                        <th><span>Order Id</span></th>
                                        <th><span>Created</span></th>
                                        
                                        <th><span>Bill amount</span></th>
                                        
                                        <th><span>Status</span></th>
                                        <th><span></span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(orders.length > 0){ for(let i=0; i <
                                        orders.length; i++ ){%>
                                    <tr>
                                        <td><%=orders[i]._id %></td>
                                        <td><%=orders[i].
                                            createdAt.toDateString() %></td>
                                       
                                        <td><%=orders[i].products.totalPrice %></td>
                                        <form action="" method="post">
                                        <td>  
                                            <%=orders[i].status %>
                                            
                                               
                                        </td>

                                        <td>
                                            <!-- Default switch -->
                                       
                                           

                                        </form>
                                        </td>
                                    </tr>
                                    <%}}else{%>
                                    <tr>
                                        <td>Coupon not found</td>
                                    </tr>
                                    <%}%>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




